// Rotation: 1.3
// Right Pitch -8 Left Pitch 7.8
start> 
:TRR=0
:TPR=0
:TRL=0
:TPL=0
:TRB=0
:TPB=0
a=401/7
b=0.28
n=90
m=180
initial> 
if :RFMD>20 then
	distance=20
else
	if :RFMD<10 then
		distance=10
	else
		distance=:RFMD
	end
end
x=2.88/(distance+4.44)
g=1<x*x
x/=1-g+x*x*g
res=-(g*(n-m*(x<0))+(1-2*g)*a*x/(1+b*x*x))
:TPR=res
:TPL=res
goto initial













