// Rotation: 1.3
// Right Pitch -8 Left Pitch 7.8
start> 
:TRR=0
:TRL=0
:TRB=0
a=401/7
b=0.28
n=90
m=180
initial> 
if :RFMD>17 then
	distance=17
else
	if :RFMD<7 then
		distance=7
	else
		distance=:RFMD
	end
end
x=0.48/(distance+3.5)
g=1<x*x
x/=1-g+x*x*g
res=g*(n-m*(x<0))+(1-2*g)*a*x/(1+b*x*x)
:TRR=res
:TRL=-res
goto initial













